{
  "name": "jukebox-cayceo",
  "version": "1.8.1",
  "description": "Rendez-vous sur JUKEBOXVR.FR",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "electron-builder",
    "watch-dev": "webpack --watch --config _builders/webpack.dev.js",
    "publish": "build -p always"
  },
  "repository": "https://github.com/davidmars/jukebox-cayceo",
  "keywords": [
    "jukeboxvr",
    "cayceo",
    "virtual reality",
    "vr"
  ],
  "author": "davidmars",
  "license": "CC0-1.0",
  "devDependencies": {
    "clean-webpack-plugin": "^0.1.18",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^0.28.9",
    "electron": "^3.1.1",
    "electron-builder": "^20.38.5",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "grunt": "^1.0.3",
    "grunt-cli": "^1.3.2",
    "grunt-contrib-clean": "^1.1.0",
    "grunt-svg-symbols": "^1.0.0",
    "grunt-svgmin": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^2.30.1",
    "install": "^0.10.4",
    "less": "^2.7.3",
    "less-loader": "^4.0.5",
    "less-plugin-autoprefix": "^1.5.1",
    "less-plugin-lists": "^1.1.1",
    "load-grunt-tasks": "^3.5.2",
    "md5-file": "^3.2.3",
    "node-sass": "^4.13.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-inline-loader": "^0.8.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.12.0",
    "webpack-cli": "^2.1.5",
    "webpack-merge": "^4.2.1"
  },
  "build": {
    "appId": "com.github.davidmars.jukebox-cayceo",
    "win": {
      "publish": [
        "github"
      ],
      "asar": true,
      "extraFiles": [
        "jukebox/**/*.html",
        "jukebox/**/*.svg",
        "jukebox/**/*.png",
        "jukebox/**/*.jpg"
      ],
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "createDesktopShortcut": "always",
      "createStartMenuShortcut": true
    }
  },
  "dependencies": {
    "adbkit": "^2.11.0",
    "electron-log": "^2.2.17",
    "electron-updater": "^4.0.0",
    "event-emitter-es6": "^1.1.5",
    "express": "^4.15.2",
    "jquery": "^3.3.1",
    "jukebox-js-libs": "github:davidmars/jukebox-js-libs#master",
    "rimraf": "^2.6.2",
    "socket.io": "^2.2.0",
    "sweetalert2": "^8.0.1"
  }
}
